layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Jay Rainey - jawrainey.me</title>
  <link href="http://jawrainey.me/atom.xml" rel="self"/>
  <link href="http://jawrainey.me/"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://jawrainey.me/</id>
  <author>
    <name>Jay Rainey</name>
    <email>hi@jawrainey.me</email>
  </author>{% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://jawrainey.me{{ post.url }}"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://jawrainey.me{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>{% endfor %}
</feed>